# Server
PORT=5000
NODE_ENV=development

# Database
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/land-marketplace

# Security
JWT_SECRET=your_super_secret_jwt_key_change_this_in_production
JWT_EXPIRE=7d
ENCRYPTION_KEY=your_32_character_encryption_key_here

# Payment (Stripe)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Payment (Monnify sandbox)
MONNIFY_CONTRACT_CODE=XXXXXXXX
MONNIFY_API_KEY=XXXXXXXX
MONNIFY_API_SECRET=XXXXXXXX
MONNIFY_SANDBOX=true

# E-sign & Maps
HELLOSIGN_API_KEY=your_hellosign_api_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Cloud Storage (Cloudinary)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Commission Defaults
PLATFORM_COMMISSION=10
ADMIN_COMMISSION=5
AGENT_COMMISSION=40
DEAL_INITIATOR_COMMISSION=45
MINIMUM_VERIFICATION_FEE=5000

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100
